{
  "pipeline": {
    "name": "userdetails-Pipeline",
    "roleArn": "arn:aws:iam::_REPLACE_ACCOUNT_NUMBER_:role/userdetails-Toolchain",
    "artifactStore": {
      "type": "S3",
      "location": "userdetails-_REPLACE_ACCOUNT_NUMBER_-pipe"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "ApplicationSource",
            "actionTypeId": {
              "category": "source",
              "owner": "AWS",
              "provider": "CodeStarSourceConnection",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "BranchName": "master",
              "ConnectionArn": "arn:aws:codeconnections:us-east-1:_REPLACE_ACCOUNT_NUMBER_:connection/_REPLACE_GITHUB_CONN_UUID_",
              "FullRepositoryId": "_REPLACE_GITHUB_ORGID_/userdetails",
              "OutputArtifactFormat": "CODE_ZIP"
            },
            "outputArtifacts": [
              {
                "name": "userdetails-SourceArtifact"
              }
            ],
            "inputArtifacts": [],
            "region": "us-east-1"
          }
        ]
      },
      {
        "name": "APPROVAL_STEP_BUILD",
        "actions": [
          {
            "name": "approveDeploy",
            "actionTypeId": {
              "category": "Approval",
              "owner": "AWS",
              "provider": "Manual",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {},
            "outputArtifacts": [],
            "inputArtifacts": []
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "CodeBuild",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ProjectName": "userdetails"
            },
            "outputArtifacts": [
              {
                "name": "userdetails-BuildArtifact"
              }
            ],
            "inputArtifacts": [
              {
                "name": "userdetails-SourceArtifact"
              }
            ]
          }
        ]
      },
      {
        "name": "APPROVAL_STEP_DEPLOY",
        "actions": [
          {
            "name": "approveDeploy",
            "actionTypeId": {
              "category": "Approval",
              "owner": "AWS",
              "provider": "Manual",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {},
            "outputArtifacts": [],
            "inputArtifacts": []
          }
        ]
      },
      {
        "name": "BuildMagic",
        "actions": [
          {
            "name": "CodeBuild",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ProjectName": "userdetails-Magic"
            },
            "outputArtifacts": [
              {
                "name": "userdetails-BuildArtifactMagic"
              }
            ],
            "inputArtifacts": [
              {
                "name": "userdetails-SourceArtifact"
              }
            ]
          }
        ]
      },
      {
        "name": "Deploy-Lambdas",
        "actions": [
          {
            "name": "LambdasGenerateChangeSet",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "CloudFormation",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ActionMode": "CHANGE_SET_REPLACE",
              "Capabilities": "CAPABILITY_NAMED_IAM",
              "ChangeSetName": "pipeline-changeset",
              "RoleArn": "arn:aws:iam::_REPLACE_ACCOUNT_NUMBER_:role/userdetails-CloudFormation",
              "StackName": "userdetails-Lambdas",
              "TemplateConfiguration": "userdetails-BuildArtifact::params/_REPLACE_ENV_/userdetails-parameters-lambdas.json",
              "TemplatePath": "userdetails-BuildArtifact::userdetails-lambdas-export.yml"
            },
            "outputArtifacts": [],
            "inputArtifacts": [
              {
                "name": "userdetails-BuildArtifact"
              }
            ],
            "region": "us-east-1"
          },
          {
            "name": "LambdasExecuteChangeSet",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "CloudFormation",
              "version": "1"
            },
            "runOrder": 2,
            "configuration": {
              "ActionMode": "CHANGE_SET_REPLACE",
              "ChangeSetName": "pipeline-changeset",
              "StackName": "userdetails-Lambdas"
            },
            "outputArtifacts": [],
            "inputArtifacts": [],
            "region": "us-east-1"
          }
        ]
      }
    ],
    "version": 4,
    "executionMode": "QUEUED",
    "pipelineType": "v2",
    "triggers": [
      {
        "providerType": "CodeStarSourceConnection",
        "gitConfiguration": {
          "sourceActionName": "ApplicationSource",
          "push": [
            {
              "branches": {
                "includes": ["master"]
              }
            }
          ]
        }
      }
    ]
  }
}
